{
  "extends": [
    "next/core-web-vitals",
    "eslint:recommended",
    "plugin:@typescript-eslint/recommended"
  ],
  "plugins": ["@typescript-eslint"],
  "rules": {
    "react/no-unescaped-entities": 0,
    "@typescript-eslint/no-unused-vars": ["error"]
  },
  "ignorePatterns": ["**/*.js", "**/*.jsx"]
}
